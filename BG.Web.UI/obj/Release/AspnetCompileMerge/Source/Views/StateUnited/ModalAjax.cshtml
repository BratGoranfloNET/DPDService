    <section class="panel mt-md">

        <h3 class="mt-none">История статусов</h3>
       

        <div class="panel-body">

            <table id="statehistoryTable" class="table table-hover">
                <thead>
                    <tr>

                        @*<th>#</th>*@

                        <th>@StateUnitedResources.dpdOrderNr</th>
                        <th>@StateUnitedResources.newState</th>
                        <th>@StateUnitedResources.transitionTime</th>
                        <th>@StateUnitedResources.terminalCode</th>
                        <th>@StateUnitedResources.terminalCity</th>

                        @*<th>@StateUnitedResources.incidentCode</th>
                          <th>@StateUnitedResources.incidentName</th>
                          <th>@StateUnitedResources.consignee</th>*@


                    </tr>
                </thead>
             
