@model DashboardIndexViewModel

<!-- Meta -->
@{
    /**/

    /**/

    Model.IsOverlayLoaderEnabled = true;

    Model.PageLabel = DashboardResources.PageLabel;
    Model.SectionLabel = DashboardResources.SectionLabel;

